{
  "name": "nuxt-app",
  "private": true,
  "type": "module",
  "scripts": {
    "build": "nuxt build",
    "dev": "concurrently \"nuxt dev --hostname 0.0.0.0 --port 3000\" \"npm run watch:tailwindcss\" \"npm run watch:pdf-css\"",
    "generate": "nuxt generate",
    "preview": "nuxt preview",
    "postinstall": "nuxt prepare",
    "watch:tailwindcss": "tailwindcss -i ./assets/tailwind.css -o ./public/styles.css --watch",
    "watch:pdf-css": "tailwindcss -c ./tailwind.pdf.config.js -i ./assets/tailwind.css -o ./public/styles-pdf.css --watch",
    "build:tailwindcss": "tailwindcss -i ./assets/tailwind.css -o ./public/styles.css --minify",
    "build:pdf-css": "tailwindcss -c ./tailwind.pdf.config.js -i ./assets/tailwind.css -o ./public/styles-pdf.css --minify",
    "test": "vitest"
  },
  "dependencies": {
    "@fancyapps/ui": "^6.0.34",
    "@pinia/nuxt": "^0.11.2",
    "daisyui": "^5.0.50",
    "nuxt": "^4.0.0",
    "pinia": "^3.0.3",
    "vue": "^3.5.17",
    "vue-grid-layout-v3": "^3.1.2",
    "vue-router": "^4.5.1"
  },
  "devDependencies": {
    "@nuxtjs/tailwindcss": "^6.14.0",
    "@vue/test-utils": "^2.4.6",
    "autoprefixer": "^10.4.21",
    "concurrently": "^9.2.0",
    "postcss": "^8.5.6",
    "sass": "^1.89.2",
    "sass-embedded": "^1.89.2",
    "tailwindcss": "^3.4.17",
    "vitest": "^3.2.4"
  }
}
